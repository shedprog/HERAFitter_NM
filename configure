#endif
void omp_set_num_threads(int);
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
#ifdef FC_DUMMY_MAIN
#ifndef FC_DUMMY_MAIN_EQ_F77
#  ifdef __cplusplus
     extern "C"
#  endif
   int FC_DUMMY_MAIN() { return 1; }
#endif
#endif
int
main ()
{
const int N = 100000;
  int i, arr[N];

  omp_set_num_threads(2);

  #pragma omp parallel for
  for (i = 0; i < N; i++) {
    arr[i] = i;
  }
  ;
  return 0;
}
